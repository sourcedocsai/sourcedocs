  "author": "SourceDocs.ai",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/sourcedocsai/sourcedocs-mcp"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.24.3"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "ts-node": "^10.9.0",
    "typescript": "^5.3.0"
  }
}
